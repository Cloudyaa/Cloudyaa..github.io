{
  "env": {
    "browser": true,
    "es2021": true
    // "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "react",
    "react-hooks"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "camelcase": "warn",
    "quotes": [
      "warn",
      "single"
    ],
    "no-duplicate-imports": "error"
  }
}
